{
  "name": "pc-form-generator",
  "version": "2.2.0",
  "description": "",
  "author": "CDS",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "cross-env nodemon --exec npm run dev:dist",
    "dev:dist": "webpack --mode=development && cross-env NODE_ENV=development  node ./bin/www",
    "start": "npm run build && cross-env NODE_ENV=production node ./bin/www",
    "lint": "node node_modules/eslint/bin/eslint.js config utils routes bin/www app.js",
    "prettier": "prettier --write '**/*.{ts,js,css,html}'",
    "test": "node node_modules/jest/bin/jest.js --coverage --env node",
    "test:watch": "node node_modules/jest/bin/jest.js --watch",
    "postinstall": "patch-package",
    "translate": "node utils/i18n-helper.js",
    "aws": "cd cdk && cdk synth && node deploy",
    "aws-bootstrap": "node cdk/bootstrap.js",
    "aws-destroy": "node cdk/destroy.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm test"
    }
  },
  "dependencies": {
    "@cdssnc/webpack-starter": "^2.0.2",
    "@csstools/postcss-sass": "^4.0.0",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^1.4.0",
    "copy-template-dir": "^1.4.0",
    "cross-env": "^6.0.3",
    "cssnano": "^4.1.11",
    "csurf": "^1.11.0",
    "dotenv": "^8.6.0",
    "dynamoose": "^1.11.1",
    "express": "^4.17.3",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2",
    "helmet": "^3.23.3",
    "i18n": "^0.8.6",
    "memorystore": "^1.6.7",
    "mini-css-extract-plugin": "^0.8.0",
    "node-pandoc": "^0.3.0",
    "node-sass": "^4.14.1",
    "node-sass-middleware": "^0.11.0",
    "normalize.css": "^8.0.1",
    "notifications-node-client": "^4.9.0",
    "nunjucks": "^3.2.3",
    "request": "^2.88.2",
    "tailwindcss": "^1.9.6"
  },
  "devDependencies": {
    "@aws-cdk/aws-docdb": "1.16.3",
    "@aws-cdk/aws-ec2": "1.16.3",
    "@aws-cdk/aws-ecs": "1.16.3",
    "@aws-cdk/aws-ecs-patterns": "1.16.3",
    "@aws-cdk/core": "1.16.3",
    "acorn": "^7.4.1",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^3.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-standard": "^4.1.0",
    "fse": "^4.0.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "patch-package": "^6.4.7",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "supertest-session": "^4.1.0",
    "webpack-cli": "^3.3.12"
  },
  "nodemonConfig": {
    "ext": "js,json,njk,scss",
    "ignore": [
      "public/dist/**/*.*"
    ]
  }
}
